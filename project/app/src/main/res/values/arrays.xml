        <item>:-)</item>   <!-- 0: Happy -->
        <item>:-(</item>   <!-- 1: Sad -->
        <item>;-)</item>   <!-- 2: Winking -->
        <item>:-P</item>   <!-- 3: Tongue sticking out -->
