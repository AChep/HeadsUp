<resources>

    <!-- Feedback -->
    <string-array name="feedback_types">
        <item>Issue</item>
        <item>Suggestion</item>
        <item>Other</item>
    </string-array>

    <!-- Privacy modes -->
    <string-array name="privacy_mode">
        <item>Hide notifications\' content</item>
        <item>Hide notifications\' actions</item>
    </string-array>
    <string-array name="privacy_mode_entries" translatable="false">
        <item>1</item>
        <item>2</item>
    </string-array>

    <!-- Notifies' priority -->
    <string-array name="notify_priorities">
        <item>Critical and urgent (missed calls, directions, emergency alerts)</item>
        <item>Important (communications, important mail)</item>
        <item>Normal (+notifications from old apps)</item>
        <item>Non-important (software updates, junk mail)</item>
        <item>Contextual and background info (weather, expired events, suggestions)</item>
    </string-array>
    <string-array name="notify_priorities_entries" translatable="false">
        <item>2</item>
        <item>1</item>
        <item>0</item>
        <item>-1</item>
        <item>-2</item>
    </string-array>

    <!-- Heads up styling -->
    <string-array name="themes">
        <item>Light</item>
        <item>Dark</item>
    </string-array>
    <string-array name="themes_entries" translatable="false">
        <item>light</item>
        <item>dark</item>
    </string-array>

    <!-- Swipe to -->
    <string-array name="st_actions">
        <item>Dismisses notification</item>
        <item>Hides notification</item>
    </string-array>
    <string-array name="st_actions_entries" translatable="false">
        <item>0</item>
        <item>1</item>
    </string-array>

